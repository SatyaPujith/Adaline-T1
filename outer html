111daa6e0dc9\",\"media\":{\"id\":1441789,\"alt\":\"\",\"name\":\"\",\"focus\":\"\",\"title\":\"\",\"source\":\"\",\"filename\":\"https://a-us.storyblok.com/f/1023026/1288x1130/586f220966/llm-as-judge.png\",\"copyright\":\"\",\"fieldtype\":\"asset\",\"meta_data\":{},\"is_external_url\":false},\"width\":\"normal\",\"caption\":{\"type\":\"doc\",\"content\":[{\"type\":\"paragraph\"}]},\"component\":\"media\"}]}},{\"type\":\"paragraph\",\"content\":[{\"text\":\"While working with the LLM rubric, you are required to:\",\"type\":\"text\"}]},{\"type\":\"blok\",\"attrs\":{\"id\":\"4ac96331-7062-4a61-8729-b6a3e0fc4085\",\"body\":[{\"_uid\":\"i-b59117ab-5663-46e8-80e7-8edba2868954\",\"media\":{\"id\":1441797,\"alt\":\"\",\"name\":\"\",\"focus\":\"\",\"title\":\"\",\"source\":\"\",\"filename\":\"https://a-us.storyblok.com/f/1023026/1894x266/1f22dc0954/llm-rubric.png\",\"copyright\":\"\",\"fieldtype\":\"asset\",\"meta_data\":{},\"is_external_url\":false},\"width\":\"normal\",\"caption\":{\"type\":\"doc\",\"content\":[{\"type\":\"paragraph\"}]},\"component\":\"media\"}]}},{\"type\":\"ordered_list\",\"attrs\":{\"order\":1},\"content\":[{\"type\":\"list_item\",\"content\":[{\"type\":\"paragraph\",\"content\":[{\"text\":\"Provide a \",\"type\":\"text\"},{\"text\":\"title\",\"type\":\"text\",\"marks\":[{\"type\":\"bold\"}]},{\"text\":\" for your evaluation.\",\"type\":\"text\"}]}]},{\"type\":\"list_item\",\"content\":[{\"type\":\"paragraph\",\"content\":[{\"text\":\"Select the dataset\",\"type\":\"text\",\"marks\":[{\"type\":\"bold\"}]},{\"text\":\" that will serve as the \",\"type\":\"text\"},{\"text\":\"ground truth\",\"type\":\"text\",\"marks\":[{\"type\":\"bold\"}]},{\"text\":\" for the LLM to evaluate the output. \",\"type\":\"text\"}]}]},{\"type\":\"list_item\",\"content\":[{\"type\":\"paragraph\",\"content\":[{\"text\":\"Describe\",\"type\":\"text\",\"marks\":[{\"type\":\"bold\"}]},{\"text\":\" the evaluation criteria.\",\"type\":\"text\"}]}]}]},{\"type\":\"paragraph\",\"conte